<% if @errors.present? %>	
	<ul>
	<% @errors.each do |msg| %>
	  <li><%= msg %></li>
	<% end %>
	</ul>
	</br>
<% end %>

<%= form_for @booking, url: listing_bookings_path do |form| %>
  <%= render partial: '/bookings/form', object: form %>

  <div class="submit-field">
    <%= form.submit %>
  </div>
<% end %>